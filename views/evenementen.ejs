<%- include('./partials/head') %>
<h2 class="event-header">Evenementen</h2>
<div class="event-container">
  <!-- checked kan je alleen toevoegen aan type radio en type checkbox -->
  <!-- input type kan niet gestyled worden daarom staat er een span achter -->
  <div class="buttons">
    <input type="radio" name="slider" id="item-1" checked /><span></span>

    <input type="radio" name="slider" id="item-2" /><span></span>

